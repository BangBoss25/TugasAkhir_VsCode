								<li class="nav-item">
									<a class="nav-link" asp-area="Owner" asp-controller="Chart" asp-action="Index" onclick="onclickTabMenu(this)">
										<i class="typcn typcn-chart-pie-outline menu-icon"></i>
										<span class="menu-title">Grafik Penjualan</span>
									</a>
								</li>
							}
							else
							{
								<li class="nav-item">
									<a class="nav-link" asp-area="Admin" asp-controller="Home" asp-action="Index">
										<i class="typcn typcn-device-desktop menu-icon"></i>
										<span class="menu-title">Dashboard</span>
										@* <!-- <div class="badge badge-danger">new</div> --> *@
									</a>
